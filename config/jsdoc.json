{
	"opts": {
		"encoding": "utf8",
		"destination": "./doc/",
		"recurse": true,
		"readme": "readme.md"
	},
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": [
			"src/node/lib"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc"
		]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"includeDate": false
		}
	}
}
