{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug web",
			"args": [
				"src/node/lib/web.js"
			],
			"envFile": "${workspaceRoot}/.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug deploy",
			"args": [
				"src/node/lib/deploy.js"
			],
			"envFile": "${workspaceRoot}/.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug dataCreator",
			"args": [
				"src/node/lib/dataCreator.js"
			],
			"envFile": "${workspaceRoot}/.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug questionBuilder",
			"args": [
				"src/node/lib/questionBuilder.js"
			],
			"envFile": "${workspaceRoot}/.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug resultWriter",
			"args": [
				"src/node/lib/resultWriter.js"
			],
			"envFile": "${workspaceRoot}/.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Tests",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"args": [
				"-u",
				"tdd",
				"--timeout",
				"999999",
				"--colors",
				"--recursive",
				"${workspaceRoot}/src/node/spec"
			],
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": "Route Solver",
			"type": "java",
			"request": "launch",
			"stopOnEntry": true,
			"jdkPath": "${env:JAVA_HOME}/bin",
			"cwd": "${fileDirname}",
			"mainClass": "com.financialforce.routesolver.RouteSolver",
			"classpath": [
				".",
				"${fileDirname}"
			]
		}
	]
}